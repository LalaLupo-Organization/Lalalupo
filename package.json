{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\"",
    "build": "next build",
    "start": "next start",
    "preinstall": "npx only-allow pnpm",
    "watch": "npx tailwindcss -i src/app/globals.css -o dist/output.css --watch",
    "kill": "killall -9 node java",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "cypress:open": "concurrently \"npm run dev\" \"cypress open\" \"npm run them:watch\"",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "prepare": "husky install",
    "format": "prettier --no-editorconfig --write \"{*.js,app/*.js,src/**/*.{js,jsx,ts,tsx},admin/src/**/*.{js,jsx,ts,tsx}}\"",
    "generate": "shopify app generate",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint ."
  },
  "engines": {
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm",
    "pnpm": ">= 8.0.0"
  },
  "dependencies": {
    "@clerk/clerk-react": "^4.30.10",
