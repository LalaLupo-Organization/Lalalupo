    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^9.0.0",
    "@next/eslint-plugin-next": "^14.2.1",
    "@types/google-libphonenumber": "^7.4.30",
    "@types/negotiator": "^0.6.1",
