{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\"",
    "build": "next build",
    "start": "next start",
    "watch": "npx tailwindcss -i src/app/globals.css -o dist/output.css --watch",
    "lint": "next lint",
    "kill": "killall -9 node java",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "cypress:open": "concurrently \"npm run dev\" \"cypress open\" \"npm run them:watch\"",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "prepare": "husky install",
    "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
  },
  "dependencies": {
    "@clerk/nextjs": "^4.29.5",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource/nunito": "^5.0.16",
    "@fontsource/open-sans": "^5.0.22",
    "@formatjs/intl-localematcher": "^0.5.4",
    "@fullstory/browser": "^2.0.2",
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.1.1",
    "@lottiefiles/react-lottie-player": "^3.5.3",
    "@next/bundle-analyzer": "^14.1.0",
    "@next/font": "^14.1.3",
    "@reduxjs/toolkit": "^2.1.0",
    "@sanity/document-internationalization": "^2.0.3",
    "@sanity/image-url": "^1.0.2",
    "@sanity/vision": "^3.27.1",
    "@tailwindcss/forms": "^0.5.7",
    "firebase": "^10.8.0",
    "framer-motion": "^11.0.3",
    "google-libphonenumber": "^3.2.34",
    "html-react-parser": "^5.1.2",
    "lottie-react": "^2.4.0",
    "mongodb": "^6.3.0",
    "mongoose": "^8.2.0",
    "negotiator": "^0.6.3",
    "next": "14.1.0",
    "next-sanity": "^7.0.17",
    "react": "^18",
    "react-dom": "^18",
    "react-fullstory": "^1.4.0",
    "react-icons": "^5.0.1",
    "react-international-phone": "^4.2.2",
    "react-redux": "^9.1.0",
    "regex-parser": "^2.3.0",
    "sanity": "^3.27.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@types/google-libphonenumber": "^7.4.30",
    "@types/negotiator": "^0.6.1",
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.52",
    "@types/react-dom": "^18.2.18",
    "@types/uuid": "^9.0.8",
    "autoprefixer": "^10.0.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "husky": "^8.0.0",
    "postcss": "^8",
    "prettier": "^3.1.1",
    "svg-inline-loader": "^0.8.2",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "validate-branch-name": "^1.3.0"
  },
  "author": "lalalupo",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern \"{src/**/*.{js,jsx,ts,tsx}}\""
    }
  }
